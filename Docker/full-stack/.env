# User/Group Settings
PUID=1000
PGID=1000
UMASK=022

# Timezone
TZ=Your/Timezone

# Base Paths
CONFIG_BASE=/path/to/your/config
DOWNLOAD_PATH=/path/to/your/downloads
MEDIA_PATH=/path/to/your/media
BACKUP_PATH=/path/to/your/backups

# Network Settings
LAN_NETWORK=your.network.range/24

# Plex Settings
PLEX_PORT=32400
PLEX_CLAIM_TOKEN=claim-<PLEX_CLAIM_TOKEN>
PLEX_ADVERTISE_URL=https://<your_plex_advertise_url>

# Application Ports
LIDARR_PORT=8686
SONARR_PORT=8989
RADARR_PORT=7878
PROWLARR_PORT=9696
SABNZBD_PORT=8086
QBITTORRENT_PORT=8080
OVERSEERR_PORT=5055
TAUTULLI_PORT=8181
PORTAINER_PORT=9001
DOZZLE_PORT=7007
WIZARR_PORT=5690

# Kometa Settings
KOMETA_PORT=9000
KOMETA_CONFIG=/config/config.yml
KOMETA_TIME=03:00
KOMETA_RUN=False
KOMETA_TEST=False
KOMETA_NO_MISSING=False

# Database Settings
POSTGRES_PORT=5432
POSTGRES_USER=<POSTGRES_USER>
POSTGRES_PASSWORD=<POSTGRES_PASSWORD>
POSTGRES_DB=<POSTGRES_DB>

# VPN Settings (for qBittorrent)
WIREGUARD_PRIVATE_KEY=<WIREGUARD_PRIVATE_KEY>
WIREGUARD_ADDRESSES=<WIREGUARD_ADDRESS>/32

# Watchtower Settings
WATCHTOWER_SCHEDULE=0 0 4 * * *

# Health Check Settings
HEALTH_CHECK_INTERVAL=30s
HEALTH_CHECK_TIMEOUT=10s
HEALTH_CHECK_RETRIES=3
HEALTH_CHECK_START_PERIOD=30s

# Unpackerr Settings
UNPACKERR_SONARR_API_KEY=<UNPACKERR_SONARR_API_KEY>
UNPACKERR_SONARR_URL=http://sonarr:8989
UNPACKERR_RADARR_API_KEY=<UNPACKERR_RADARR_API_KEY>
UNPACKERR_RADARR_URL=http://radarr:7878
UNPACKERR_LIDARR_API_KEY=<UNPACKERR_LIDARR_API_KEY>
UNPACKERR_LIDARR_URL=http://lidarr:8686
UNPACKERR_PROWLARR_API_KEY=<UNPACKERR_PROWLARR_API_KEY>
UNPACKERR_PROWLARR_URL=http://prowlarr:9696
UNPACKERR_READARR_API_KEY=<UNPACKERR_READARR_API_KEY>
UNPACKERR_READARR_URL=http://readarr:8787