# Queue Cleaner Configuration

# Enable or disable the script
ENABLE_QUEUE_CLEANER=true

# How often the script should run
QUEUE_CLEANER_INTERVAL=15m

# Radarr Instances (space-separated list)
RADARR_INSTANCES="Radarr Radarr_4K"

# Sonarr Instances (space-separated list)
SONARR_INSTANCES="Sonarr Sonarr_4K Sonarr_Anime"

# Readarr Instances (space-separated list)
READARR_INSTANCES="Readarr Readarr_Audiobooks"

# Lidarr Instances (space-separated list)
LIDARR_INSTANCES="Lidarr Lidarr_Lossless"

# Radarr Configuration
RADARR_Radarr_URL=http://radarr:7878
